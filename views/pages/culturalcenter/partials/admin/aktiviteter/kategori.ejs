<div class="category container">
    
        <%= buildingData[0].building_id %>           


    <% arrangementData.forEach(function(elm) { %>

        <% if(elm.arrangement_hide == 1){ %>
            <%= console.log(elm.arrangement_id) %>
            <div class="categoryItem">
                    <h4>Tittel</h4>
                    <input type="text" id="input_title_<%= elm.arrangement_id %>" value="<%= elm.arrangement_title %>">
                    <img src="/static/resources/pages/cultural_center/media/aktiviteter/<%= elm.arrangement_imgPath %>" alt="" class="categoryImg">
                    <h4>Billede</h4>
                    <form ref='uploadForm' id='uploadForm' action='http://localhost:1337/json/arrangement/create/activity/upload_img' method='post' encType="multipart/form-data">
                        <input type="file" name="sampleFile" />
                        <input type='submit' value='Upload!' />
                  </form>    
                    <h4>Text</h4>
                    <textarea name="text" id="input_text_<%= elm.arrangement_id %>" cols="30" rows="5"><%= elm.arrangement_text %></textarea>
                    <h4>Adresse</h4>
                    <select name="" id="input_adress_<%= elm.arrangement_id %>" >
                        
                        <% adressData.forEach(function(element) { %>
        
                            <option value="<%= element.adress_id %>" 
                                <% if(element.adress_id == elm.fk_adress){ %>
                                    <%= selected="selected" %>                                                 
                                <% } %> >
                                <%= element.adress_name %>           
                            </option>
        
                        <% }) %>
        
                    </select>
                    
                    <h4>Bygnings Nummer</h4>
                    
                    <select name="" id="input_buildingNo_<%= elm.arrangement_id %>">
        
                        <% buildingData.forEach(function(building_elm) { %>
        
                            <option value="<%= building_elm.building_id %>" 
                                <% if(building_elm.building_id == elm.fk_buildingNo){ %>
                                    <%= selected="selected" %>                                                 
                                <% } %> >
                                <%= building_elm.building_number %>           
                            </option>
        
                        <% }) %>
        
                    </select>
        
                    <h4>Event Start</h4>
                    <p class="date">
                        <%= elm.arrangement_dateStart %>
                    </p>
                    <input type="text" id="input_dateStart_<%= elm.arrangement_id %>" value="<%= elm.arrangement_dateStart %>">
                    <h4>Event Slut</h4>
                    <p class="date">
                        <%= elm.arrangement_dateEnd %>
                    </p>
                    <input type="text" id="input_dateEnd_<%= elm.arrangement_id %>" value="<%= elm.arrangement_dateEnd %>">
                    <h4>Pris</h4>
                    <input type="text" id="input_price_<%= elm.arrangement_id %>" value="<%= elm.arrangement_price %>">
                    <h4>Vis = 1 | Skjul = 0</h4>
                    <input type="text" id="input_show_<%= elm.arrangement_id %>" value="<%= elm.arrangement_hide %>">
                    <h4>Skift Kategori eller Slet Arrangement</h4>
                    <select name="" id="input_category_<%= elm.arrangement_id %>">

                        <% allCategoryData.forEach(function(category_elm) { %>

                            <option value="<%= category_elm.category_id %>" 
                                <% if(category_elm.category_id == elm.fk_type_id){ %>
                                    <%= selected="selected" %>                                                 
                                <% } %> >
                                <%= category_elm.category_title %>           
                            </option>
        
                        <% }) %>

                    </select>   
                </div>
                <button onclick="gem('<%= elm.arrangement_id %>');">gem</button>
            <% } %>

        
        
        <% }); %>
</div>